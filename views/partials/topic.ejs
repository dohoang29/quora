<div class="col">
        <ul class="list-unstyled">
          <% topics.forEach((topic)=>{ %>
          <li class="media my-3">
            <img
              class="mr-2 topic-image"
              src="<%= topic.image %>"
              alt="<%= topic.title %>"
            />
            <div class="media-body"><p class="mt-0 mb-1 topic-title"><%= topic.title %></p></div>
            
          </li>
          <% }) %>
        </ul>
      </div>